{
  "name": "vrchat-exif-writer",
  "type": "module",
  "version": "0.0.1",
  "description": "add EXIF tag to VRChat Pics.",
  "repository": {
    "type": "github",
    "url": "https://github.com/m-hayabusa/VRChat-Exif-Writer.git"
  },
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "tsc": "tsc",
    "unregist": "powershell -C \"Start-Process powershell -Verb runAs -ArgumentList '-C Unregister-ScheduledTask -Confirm:$false -TaskName VRChat-Exif-Writer; sleep -s 1'",
    "regist": "powershell -C \"Start-Process powershell -Verb runAs -ArgumentList '-C','Register-ScheduledTask','-TaskName','VRChat-Exif-Writer','-TaskPath','nekomimiStudio','-Trigger','$(New-ScheduledTaskTrigger -AtLogOn -User $((Get-WMIObject -class Win32_ComputerSystem).UserName))','-Principal','$(New-ScheduledTaskPrincipal -LogonType S4U -RunLevel Limited -UserId ((Get-WMIObject -class Win32_ComputerSystem).UserName))','-Action',$(Invoke-Expression `'\"'$(New-ScheduledTaskAction -WorkingDirectory '$(Get-Location)' -Execute utils\\service.bat)'`'),'-Force',';sleep','-s 1'"
  },
  "author": "m-hayabusa (https://mewl.me/@mewl)",
  "license": "MIT",
  "dependencies": {
    "exiftool.exe": "^10.53.0",
    "node-osc": "^8.0.3",
    "tail": "^2.2.4",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@types/node-osc": "^6.0.0",
    "@types/tail": "^2.2.1"
  }
}